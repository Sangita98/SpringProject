package test_hibernate;

public class Employee {
	int id;
	String ffirstName;
	String lastName;
	int salary;
	
	Employee(){}

	public Employee(int id, String ffirstName, String lastName, int salary) {
		super();
		this.id = id;
		this.ffirstName = ffirstName;
		this.lastName = lastName;
		this.salary = salary;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getFfirstName() {
		return ffirstName;
	}

	public void setFfirstName(String ffirstName) {
		this.ffirstName = ffirstName;
	}

	public String getLastName() {
		return lastName;
	}

	public void setLastName(String lastName) {
		this.lastName = lastName;
	}

	public int getSalary() {
		return salary;
	}

	public void setSalary(int salary) {
		this.salary = salary;
	}

	@Override
	public String toString() {
		return "Employee [id=" + id + ", ffirstName=" + ffirstName + ", lastName=" + lastName + ", salary=" + salary
				+ "]";
	}
	
	

		
}
